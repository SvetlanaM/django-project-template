[MASTER]
load-plugins=pylint_django
ignore=migrations

[FORMAT]
max-line-length=120

[MESSAGES CONTROL]
disable=missing-docstring,invalid-name,logging-fstring-interpolation
  W0107,  # Unnecessary pass statement (unnecessary-pass)
  W0511,C0301,C0302,R0402,W0401,W0614,W0223,W1514,R0801,R0903,E5110,
  W0613,R0913,R0912,R0904,W0105,R0914,R1710,C0113,W0707

[DESIGN]
max-parents=13

[TYPECHECK]
generated-members=REQUEST,acl_users,aq_parent,"[a-zA-Z]+_set{1,2}",save,delete
